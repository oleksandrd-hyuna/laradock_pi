{
  "name": "god_mode",
  "version": "1.0.1",
  "description": "God Mode API in NodeJS",
  "main": "server.js",
  "scripts": {
    "start": "supervisor -i public/resources -- server.js | bunyan",
    "start-debug": "supervisor -i public/resources -- --debug=7070 server.js | bunyan",
    "qa": "supervisor -- server.js --conf=./config/default-qa | bunyan",
    "watch": "gulp watch",
    "postinstall": "bower install",
    "postupdate": "bower update",
    "test-server": "supervisor -i public/resources -- --debug=7070 test/server_test.js | bunyan",
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- -R spec test/test.js --timeout 10000 --env=test --conf=./config/default-travis"
  },
  "bugs": {
    "url": "https://github.com/JoMedia/god_mode/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JoMedia/god_mode.git"
  },
  "author": "JoMedia",
  "license": "ISC",
  "dependencies": {
    "app-module-path": "^1.0.2",
    "async": "^1.2.1",
    "aws-sdk": "^2.1.38",
    "bluebird": "^3.4.1",
    "body-parser": "^1.13.2",
    "bower": "^1.4.1",
    "bunyan": "^1.4.0",
    "colors": "^1.1.0",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.3.5",
    "date-util": "^1.2.1",
    "express": "^4.12.4",
    "express-session": "^1.11.3",
    "fs-extra": "^0.23.1",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-minify-css": "^1.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.5",
    "html-entities": "^1.2.0",
    "jade": "^1.10.0",
    "json-override": "^0.2.0",
    "middleware": "^1.0.0",
    "minimist": "^1.1.1",
    "mocha": "^2.2.5",
    "moment": "^2.10.3",
    "moment-timezone": "*",
    "node-validator": "^0.1.13",
    "path": "^0.11.14",
    "redis": "^0.12.1",
    "request": "^2.57.0",
    "require": "^2.4.18",
    "requirejs": "^2.1.18",
    "s3": "^4.4.0",
    "statsd-client": "^0.2.1",
    "strformat": "0.0.7",
    "use": "^1.0.3",
