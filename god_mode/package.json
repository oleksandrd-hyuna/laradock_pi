  },
  "devDependencies": {
    "chai": "^3.2.0",
    "gulp-include": "^2.3.1",
    "gulp-jshint": "^2.0.0",
    "gulp-sass": "^2.3.0",
    "istanbul": "^0.3.18",
    "jshint": "^2.9.1",
    "json-stable-stringify": "^1.0.1",
    "jsonschema": "^1.0.2",
    "merge-stream": "^1.0.0",
    "nock": "^8.0.0",
    "vinyl": "^1.1.1",
    "zombie": "^4.2.1"
  }
}
